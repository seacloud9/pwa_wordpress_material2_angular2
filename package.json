{
  "name": "ng-wordpress",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "ng build",
    "build--prod": "ng build --prod --aot --no-sourcemap && npm run sw",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "disk-build": "ng build && npm run sw",
    "analyze-bundles": "cp -f webpack.config.js node_modules/angular-cli/models/webpack.config.js && npm run build--prod && cd dist && webpack-bundle-analyzer stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "^1.0.0-beta.31",
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/compiler-cli": "^2.4.7",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/material": "2.0.0-beta.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@angular2-material/core": "2.0.0-alpha.8-2",
    "@angular2-material/icon": "2.0.0-alpha.8-2",
    "@ngtools/webpack": "^1.2.9",
    "aframe": "^0.5.0",
    "aframe-html-shader": "^0.1.1",
    "angular2-infinite-scroll": "^0.3.1",
    "blueprint-css": "2.0.0-beta.5",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "mdi": "^1.8.36",
    "ng2-lazyload-image": "2.4.0",
    "ng2-responsive": "^0.8.3",
    "ng2-social-share": "0.0.10",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.4",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "sass-lint": "1.10.2",
    "sw-precache": "4.3.0",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-stats-plugin": "^0.1.4"
  }
}
